import os, pathlib
junit_prj_base = "C:\\Users\\jimuc\\eclipse-workspace\\junit\\src\\main\\java\\com\\test\\junit\\"
asgs_base = "C:\\asgs\\"
file_name = "App.java"

def copy_file_and_test():

asg_list = os.listdir(asgs_base)
for i in asg_list:
    stu_number = i
    stu_file_path = asgs_base + i + "\\" + file_name
    print(stu_file_path)
    stu_file = pathlib.Path(stu_file_path)
    if stu_file.is_file():
        print("File exists under path " + i)
    else:
        print("File not exists under path " + i)
        
